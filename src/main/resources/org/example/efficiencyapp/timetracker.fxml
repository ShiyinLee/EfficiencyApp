<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
           stylesheets="@timetracker.css" fx:controller="org.example.efficiencyapp.timetrackerController">
    <Label layoutX="220.0" layoutY="44.0" text="Time Tracker">
      <font>
         <Font name="Times New Roman Bold Italic" size="30.0" />
      </font></Label>
    <Label fx:id="timeLabel" layoutX="240.0" layoutY="167.0" text="00:00:00">
      <font>
         <Font name="Times New Roman Bold" size="35.0" />
      </font></Label>
    <HBox alignment="CENTER" spacing="10" />
    <Button layoutX="476.0" layoutY="409.0" onAction="#GoToMenu" text="back to menu">
      <font>
         <Font name="Times New Roman" size="15.0" />
      </font></Button>
     <Button fx:id="startStopButton" layoutX="179.0" layoutY="277.0" onAction="#onStartStopClicked" text="Start">
      <font>
         <Font name="Times New Roman Bold" size="20.0" />
      </font>
   </Button>
     <Button fx:id="resetButton" layoutX="386.0" layoutY="277.0" onAction="#onResetClicked" text="Reset">
      <font>
         <Font name="Times New Roman Bold" size="20.0" />
      </font>
   </Button>
</AnchorPane>
